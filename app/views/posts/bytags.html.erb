<table>
  <tr>
    <th>Data</th>
    <th>Title</th>
    <th>Status</th>
    <th>ifTop</th>
    <th>TextContent</th>
    <th>Img</th>

  </tr>

  <% for post in @array %>
    <tr>
      <td><%=h post.data %></td>
      <td><%=h post.title %></td>
      <td><%=h post.status %></td>
      <td><%=h post.ifTop %></td>
      <td><%=h post.text_content %></td>
      <td><%=h post.img_route %></td>
    </tr>
  <% end %>
</table>

